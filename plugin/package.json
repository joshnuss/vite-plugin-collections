{
  "name": "vite-plugin-collections",
  "version": "1.0.0-beta.3",
  "description": "A vite plugin for working with directories of markdown",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write --cache --parser typescript \"src/**/*.ts\"",
    "check": "tsc --noEmit",
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "pkg:lint": "publint"
  },
  "keywords": [
    "vite",
    "markdown",
    "unified",
    "remark",
    "rehype"
  ],
  "author": "Josh Nussbaum (http://github.com/joshnuss)",
  "license": "MIT",
  "packageManager": "pnpm@10.13.1",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "@types/node": "^24.1.0",
    "@types/pluralize": "^0.0.33",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.32.0",
    "prettier": "^3.6.2",
    "publint": "^0.3.12",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vfile": "^6.0.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "pluralize": "^8.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-html": "^16.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "remark-stringify": "^11.0.0",
    "rollup": "^4.46.1",
    "scule": "^1.3.0",
    "unified": "^11.0.5",
    "vfile-matter": "^5.0.1",
    "vite": "^7.0.6",
    "zod": "^4.0.10"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "package.json",
    "README.md",
    "dist/*"
  ]
}
