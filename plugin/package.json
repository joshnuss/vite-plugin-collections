{
  "name": "vite-plugin-collections",
  "version": "1.0.0-beta.0",
  "description": "A vite plugin for working with directories of markdown",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "check": "tsc --noEmit",
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "keywords": [
    "vite",
    "markdown",
    "unified",
    "remark",
    "rehype"
  ],
  "author": "Josh Nussbaum (http://github.com/joshnuss)",
  "license": "MIT",
  "packageManager": "pnpm@10.13.1",
  "devDependencies": {
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "@types/node": "^24.1.0",
    "@types/pluralize": "^0.0.33",
    "@vitest/coverage-v8": "^3.2.4",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vfile": "^6.0.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "pluralize": "^8.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-html": "^16.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "remark-stringify": "^11.0.0",
    "scule": "^1.3.0",
    "unified": "^11.0.5",
    "vfile-matter": "^5.0.1",
    "zod": "^4.0.10"
  }
}
