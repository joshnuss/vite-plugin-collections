{
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.12.0"
  },
  "homepage": "https://github.com/joshnuss/vite-plugin-collections",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshnuss/vite-plugin-collections"
  },
  "keywords": [
    "vite",
    "markdown",
    "unified",
    "remark",
    "rehype"
  ],
  "scripts": {
